{
  "name": "starter-express-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "development": "cross-env NODE_ENV=development nodemon -L",
    "start": "cross-env NODE_ENV=production node dist/server",
    "sq:cms": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "format": "prettier --write src/**/*.ts --config ./.prettierrc",
    "build": "tsc --build --force && tsc-alias && copyfiles -u 1 \"src/views/**/*\" dist/",
    "tailwind:css": "postcss ./public/styles/tailwind.css -o ./public/styles/style.css -w",
    "clean": "rimraf ./dist/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/connect-livereload": "^0.6.0",
    "@types/express": "^4.17.17",
    "@types/livereload": "^0.9.2",
    "@types/morgan": "^1.9.4",
    "connect-livereload": "^0.6.1",
    "copyfiles": "^2.4.1",
    "livereload": "^0.9.3",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "postcss-cli": "^10.1.0",
    "rimraf": "^5.0.1",
    "sequelize-cli": "^6.6.1",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "pg": "^8.11.2",
    "postcss": "^8.4.27",
    "sequelize": "^6.32.1"
  }
}
